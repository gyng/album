{
  "name": "album",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "index:update": "(cd ../index; bash ./do-full-index.sh)",
    "deploy:vercel": "npx vercel build --prod && npx vercel deploy --prebuilt --prod",
    "dev": "next dev --turbopack",
    "build": "next build && echo 'build done'",
    "start": "next start",
    "lint": "next lint && prettier --check .",
    "lint:fix": "prettier --write .",
    "test": "jest --watch",
    "test:ci": "timeout 60s jest --forceExit --runInBand && npm run lint",
    "test:e2e": "echo \"CI env: $CI\" && npm run build && playwright test",
    "test:e2e:built": "playwright test",
    "test:e2e:headed": "echo \"CI env: $CI\" && npm run build && playwright test --headed"
  },
  "dependencies": {
    "@sqlite.org/sqlite-wasm": "^3.50.4-build1",
    "@tanstack/react-query": "^5.90.12",
    "@vercel/analytics": "^1.6.1",
    "coi-serviceworker": "^0.1.7",
    "exifr": "^7.1.3",
    "fraction.js": "^5.3.4",
    "image-size": "^2.0.2",
    "maplibre-gl": "^5.14.0",
    "next": "^15.0.0",
    "react": "^19.2.1",
    "react-dom": "^19.2.1",
    "react-map-gl": "^8.1.0",
    "sharp": "^0.34.5",
    "sqlite3": "^5.1.7",
    "use-debounce": "^10.0.6",
    "usehooks-ts": "^3.1.1",
    "uuid": "^13.0.0"
  },
  "overrides": {
    "usehooks-ts": {
      "react": "^19.2.1"
    }
  },
  "devDependencies": {
    "@playwright/test": "^1.57.0",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@types/jest": "^30.0.0",
    "@types/node": "24.10.1",
    "@types/react": "19.2.7",
    "@types/react-dom": "19.2.3",
    "eslint": "9.39.1",
    "eslint-config-next": "^16.0.7",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "prettier": "^3.7.4",
    "typescript": "5.9.3"
  },
  "engines": {
    "node": "^22.0.0"
  }
}
